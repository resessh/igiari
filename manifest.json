{
  "name": "Igiari",
  "version": "0.0.1",
  "description": "Igi ari!!",
  "permissions": ["https://meet.google.com/*", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "contentscript.js",
    "sounds/metalgear-alert.wav",
    "sounds/igiari.wav",
    "sounds/matta.wav"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/igiari_icon16.png",
      "32": "images/igiari_icon32.png",
      "48": "images/igiari_icon48.png",
      "128": "images/igiari_icon128.png"
    }
  },
  "icons": {
    "16": "images/igiari_icon16.png",
    "32": "images/igiari_icon32.png",
    "48": "images/igiari_icon48.png",
    "128": "images/igiari_icon128.png"
  },
  "manifest_version": 2
}
